{
    "name": "nunjucks-parser",
    "version": "1.0.2",
    "description": "Extract dependencies from nunjucks templates",
    "main": "dist/index.js",
    "scripts": {
        "build": "shx mkdir -p ./dist && babel -o dist/index.js src/index.js",
        "build:dev": "cross-env NODE_ENV=development npm run build",
        "clean": "shx rm -rf ./dist",
        "prepublishOnly": "npm run clean && cross-env NODE_ENV=production npm run test",
        "test": "npm run build:dev && ava --verbose"
    },
    "repository": "chocolateboy/nunjucks-parser",
    "author": "chocolateboy",
    "license": "Artistic-2.0",
    "files": [
        "dist/index.js"
    ],
    "ava": {
        "files": [
            "test/*.js"
        ]
    },
    "browserslist": [
        ">0.25%",
        "firefox 56",
        "firefox esr",
        "maintained node versions",
        "not dead",
        "not ie 11",
        "not op_mini all"
    ],
    "dependencies": {
        "@babel/runtime": "^7.0.0",
        "core-js": "^3.1.4",
        "pify": "^4.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.0.0",
        "@babel/core": "^7.1.0",
        "@babel/plugin-transform-runtime": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "ava": "2.1.0",
        "cross-env": "^5.2.0",
        "nunjucks": "^3.2.0",
        "shx": "^0.3.2",
        "sprintf-js": "^1.1.1"
    },
    "keywords": [
        "bundle",
        "bundler",
        "dependencies",
        "dependency",
        "extract",
        "metadata",
        "nunjucks",
        "parcel",
        "parcel-bundler",
        "parse",
        "parser",
        "query",
        "render",
        "template",
        "templates"
    ],
    "peerDependencies": {
        "nunjucks": "^3.0"
    }
}
