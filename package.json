{
    "name": "nunjucks-parser",
    "version": "0.0.1",
    "description": "Extract dependencies and other metadata from nunjucks templates",
    "main": "dist/index.js",
    "scripts": {
        "build:dev": "shx mkdir -p ./dist && babel -o dist/index.js src/index.js",
        "build": "cross-env NODE_ENV=production npm run build:dev",
        "clean": "shx rm -rf ./dist",
        "prepublishOnly": "npm run clean && cross-env NODE_ENV=production npm run test",
        "test": "npm run build:dev && ava --verbose ./test"
    },
    "repository": "chocolateboy/nunjucks-parser",
    "author": "chocolateboy",
    "license": "Artistic-2.0",
    "private": true,
    "files": [
        "dist/index.js"
    ],
    "dependencies": {
        "@babel/runtime": "^7.0.0",
        "pify": "^4.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.0.0",
        "@babel/core": "^7.1.0",
        "@babel/plugin-transform-runtime": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "ava": "1.0.0-beta.4",
        "cross-env": "^5.2.0",
        "nunjucks": "^3.1.3",
        "shx": "^0.3.2"
    },
    "keywords": [
        "nunjucks",
        "bundle",
        "bundler",
        "dependencies",
        "dependency",
        "extract",
        "metadata",
        "parse",
        "parser",
        "render",
        "template"
    ],
    "peerDependencies": {
        "nunjucks": "^3.1.3"
    }
}
